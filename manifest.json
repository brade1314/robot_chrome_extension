{
	"name": "baiduyun",
	"description": "百度云下载",
	"permissions": ["tabs",
	"declarativeContent",
	"background",
	"storage",
	"notifications",
	"webRequest",
	"webRequestBlocking",
	"http://*/*",
	"https://*/*"
	],
	"homepage_url": "http://blog.r136.dev",
	"content_scripts": [{
		"js": ["content.js"],
		"matches": ["https://pan.baidu.com/*"],
		"run_at": "document_end"
	}],
	"web_accessible_resources": ["lib/*","img/*"],
	"version": "1.0",
	"browser_action": {
		"default_icon": "img/bdy.png",
		"default_title": "百度云下载"
	},
	"icons": {
		"48": "img/bdy.png"
	},
	"manifest_version": 2,
	"author": "brade"
}